   "source": [
    "class Solution:\n",
    "    def removeElement(self, nums, val: int) -> int:\n",
    "        unique_index = 0\n",
    "        for index, num in enumerate(nums):\n",
    "            if num == val:\n",
    "                continue\n",
    "            nums[unique_index] = num\n",
    "            unique_index += 1\n",
    "        return unique_index"
   ]
  }
